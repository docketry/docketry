{
    "name": "docketry/docketry",
    "description": "Docketry CMS. The Fast, Powerful but Simple CMS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Develop Scotland",
            "email": "hello@develop.scot"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Docketry\\Docketry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docketry\\Docketry\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Docketry\\Docketry\\Providers\\DocketryServiceProvider"
            ]
        }
    }
}
